<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".all.activity.CashierDashboardActivity">


    <FrameLayout
        android:id="@+id/fl_wrapper"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:layout_above="@+id/cashier_bottom_navigation"/>

    <com.ismaeldivita.chipnavigation.ChipNavigationBar
        android:id="@+id/cashier_bottom_navigation"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        app:cnb_menuResource="@menu/cashnavmenu"
        android:layout_alignParentBottom="true"
        android:background="@color/greytoDarkG"
        app:cnb_radius="8dp"
        app:cnb_iconSize="24dp"
        app:cnb_addBottomInset="false"
        app:cnb_addLeftInset="false"
        app:cnb_addRightInset="false"
        app:cnb_addTopInset="false"
        app:cnb_unselectedColor="@color/white"
        />

</RelativeLayout>